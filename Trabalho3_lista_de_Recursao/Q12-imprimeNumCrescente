#include <stdio.h>

int imprimeNumCrescente(int n);

int main()
{
	int n;

	printf("Digite numero:\n");
	scanf("%d", &n);
  printf("\n");
	
  imprimeNumCrescente(n);
  
	return 0;
}

int imprimeNumCrescente(int n)
{  
	if(n < 0)
	{
		return 0;
	}
	else
	{
    imprimeNumCrescente(n - 1);		    
	} 
  printf("%d\n", n);
  
  return 0; 
}
